<UserControl
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:av="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="av" x:Class="TotalMinerUnity.TotalMinerUnityMainView" MinWidth="256" MinHeight="144" ClipToBounds="True">

    <UserControl.Resources>
    </UserControl.Resources>

    <Grid MinWidth="256" MinHeight="144">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="3.3*"/>
            <ColumnDefinition Width="6.6*"/>
        </Grid.ColumnDefinitions>

        <Rectangle Grid.Column="0" Stroke="Black" Margin="10,10,0,10"/>
        <Rectangle Grid.Column="1" Stroke="Black" Margin="0,10,10,10"/>
        <Button Grid.Column="0" Content="Button" Margin="30,20,10,20" />
    </Grid>
</UserControl>
